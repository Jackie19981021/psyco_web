{
  "version": 2,
  "functions": {
    "backend/api/index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/api/index.js"
    }
  ],
  "env": {
    "JWT_SECRET": "@jwt_secret",
    "MONGODB_URI": "@mongodb_uri"
  }
}