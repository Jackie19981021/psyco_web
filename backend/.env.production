# 生产环境配置
NODE_ENV=production
PORT=5000

# MongoDB生产数据库（请替换为你的MongoDB Atlas连接字符串）
MONGODB_URI=mongodb+srv://username:password@cluster0.mongodb.net/psycho-production?retryWrites=true&w=majority

# JWT密钥（请生成一个强密钥）
JWT_SECRET=your-super-secure-jwt-secret-key-for-production

# CORS允许的前端域名
FRONTEND_URL=https://your-netlify-app.netlify.app

# 安全配置
BCRYPT_ROUNDS=12
SESSION_TIMEOUT=24h
MAX_CONNECTIONS=1000

# 数据库配置
DB_NAME=psycho-production
DB_MAX_POOL_SIZE=10
DB_TIMEOUT=5000